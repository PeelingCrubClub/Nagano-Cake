<h2>ジャンル一覧・追加</h2>
<%= form_with model: [:admin, @genre], local: true do |f| %>
  <%= f.label :name, "ジャンル" %>
  <%= f.text_field :name %>
  <%= f.submit '新規登録'%>
<% end %>

    <tr>
      <th>ジャンル</th>
    </tr>
    <tbody>
      <% @genres.each do |genre| %>
        <tr>
        <td><%= genre.name %></td>
        <td class="text-right">
          <%= link_to '編集する', edit_admin_genre_path(genre.id), class: 'btn btn-sm btn-success' %>
        </td>
        </tr>
      <% end %>
    </tbody>